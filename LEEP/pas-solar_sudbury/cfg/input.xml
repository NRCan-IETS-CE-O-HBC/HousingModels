  <step_variable>building/month</step_variable>      
  <step_variable>building/zone_01/air_point/temperature</step_variable>
  <step_variable>building/zone_01/supplied_energy/heating</step_variable>
  <step_variable>building/zone_01/surface_06/temperature</step_variable>
  <step_variable>building/zone_01/surface_16/temperature</step_variable>
  <step_variable>building/zone_01/surface_17/temperature</step_variable>
  <step_variable>building/zone_01/surface_18/temperature</step_variable>
  <step_variable>building/zone_01/surface_19/temperature</step_variable>
  <step_variable>building/zone_01/surface_20/temperature</step_variable>
  <step_variable>building/zone_01/surface_21/temperature</step_variable>
  <step_variable>building/zone_01/surface_22/temperature</step_variable>
  <step_variable>building/zone_01/surface_23/temperature</step_variable>
  <step_variable>building/zone_01/surface_24/temperature</step_variable>
  <step_variable>building/zone_01/surface_25/temperature</step_variable>
  <step_variable>building/zone_01/surface_26/temperature</step_variable>
  <step_variable>building/zone_01/surface_27/temperature</step_variable>
  <step_variable>building/zone_01/surface_28/temperature</step_variable>
  <step_variable>building/zone_01/surface_29/temperature</step_variable>
  <step_variable>building/zone_01/surface_30/temperature</step_variable>
  <step_variable>building/zone_01/surface_31/temperature</step_variable>     
  <step_variable>building/zone_02/air_point/temperature</step_variable>
  <step_variable>building/zone_02/supplied_energy/heating</step_variable>
  <step_variable>building/zone_03/air_point/temperature</step_variable>
