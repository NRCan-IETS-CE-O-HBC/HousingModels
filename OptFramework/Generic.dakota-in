
# General definitions
environment,                                                            
    output_file 'Dakota_out_msg.txt'      
    error_file  'Dakota_err_msg.txt'      
    tabular_data
    results_output results_output_file 'Dakota_output' 
    top_method_pointer 'Coliny_EA'
    

  
method,  
    id_method = 'Coliny_EA'                       
    coliny_ea
    seed 23 
    population_size 50
    initialization_type unique_random
    replacement_type chc 10
    fitness_type merit_function 
    mutation_type replace_uniform
    mutation_rate  0.8    
    output debug
    show_misc_options 
    max_function_evaluations 50000
    max_iterations 100

method     
    id_method = "random"
    adaptive_sampling 
    
    
model 
    single 
    
