interface,
    id_interface  'test_dakota'
	fork 
	   asynchronous 
	   evaluation_concurrency = 30
       parameters_file = 'dakota-choices.in'
       results_file = 'SubstitutePL-output.txt'
	   file_save
    work_directory      
       directory_tag
#	   directory_save
       copy_files "/home/ubuntu/HousingModels/OptFramework/GenericHome-2st-garage"
                  "climate_linux"
                  "OPTIONS-generic-MATTAMY-OTT.options"
    analysis_driver './substitute-alias-jtb.pl -d -vv -o OPTIONS-generic-MATTAMY-OTT.options -c dakota-choices.in -b GenericHome-2st-garage' 
	              verbatim
    
responses, 
    id_responses 'rest_responses'
    num_objective_functions 23
    descriptors 
       'EnergyTotal'  
       'UtilBillNoPVRevenueDoll'  
